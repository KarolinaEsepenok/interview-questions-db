  "type": "checkbox",
  "answers": [
    {
      "text": "`var foo = function bar() {};`",
      "isCorrect": false
    },
    {
      "text": "`;(function () {})();`",
      "isCorrect": true
    },
    {
      "text": "`function(){}();`",
      "isCorrect": false
